function ws_client_declarer_sinistre($data){
  $ip = variable_get('ip_server');
  $root = variable_get('root');
  $declarer_sinistre = variable_get('declarer_sinistre');

  $url = 'http://'. $ip .'/'. $root .'/' . $declarer_sinistre;

  $data = drupal_json_encode($data);
  //var_dump($data); die();
  $options = array(
    'method' => 'POST',
    'data' => $data,
